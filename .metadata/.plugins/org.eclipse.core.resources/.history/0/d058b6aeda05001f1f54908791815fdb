<script>
	getData();
	function getData(){
		$.ajax({
			type:"get",
			url:"https://dapi.kakao.com/v3/search/book",
			headers:{"Authorization","KakaoAK f3d977d0f78639ca211ff67b0676d64c"},
			dataType:"json",
			data:{"query":"JSP", page:1, size:6},
			success:function(data){
				console.log(data);
			}
		});
	}
</script>